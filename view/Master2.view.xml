<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" controllerName="com.transfieldservices.view.Master2">
	<Page id="master2Page" showNavButton="true" navButtonPress="onNavBack" title="Timesheet Entries">
		<subHeader>
			<Bar id="master2SearchBar">
				<contentMiddle>
					<SearchField id="master2SearchField" showRefreshButton="{device&gt;/isNoTouch}" placeholder="{i18n&gt;master2SearchPlaceholder}" search="onSearch" tooltip="{i18n&gt;master2SearchTooltip}" width="100%">
					</SearchField>
				</contentMiddle>
			</Bar>
		</subHeader>
		<content>
			<List id="master2List" items="{detailSet}" mode="{device&gt;/listMode}" noDataText="{i18n&gt;masterListNoDataText}" select="onSelect" growing="true" growingScrollToLoad="true">
				<items>
					<ObjectListItem id="master2ListItem" type="{device&gt;/listItemType}" press="onSelect" title="{Timetxt}" number="{Durationtxt}" numberUnit="">
						<attributes>
							<ObjectAttribute text="{Atttxt}"/>
						</attributes>
						<firstStatus>
							<ObjectStatus text="{Statustxt}"/>
						</firstStatus>
					</ObjectListItem>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page></mvc:View>