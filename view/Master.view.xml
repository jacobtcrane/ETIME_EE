<mvc:View controllerName="com.broadspectrum.etime.ee.view.Master" displayBlock="true" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Page id="master1Page" title="{i18n>master1Title}">
		<!--<headerContent>-->
		<!--</headerContent>-->
		<subHeader>
			<Bar id="master1SearchBar">
				<contentMiddle>
					<SearchField id="master1SearchField" placeholder="{i18n]master1SearchPlaceholder}" search="onSearch" showRefreshButton="{device]/isNoTouch}"
						tooltip="{i18n>master1SearchTooltip}" width="100%"></SearchField>
				</contentMiddle>
			</Bar>
		</subHeader>
		<content>
			<l:VerticalLayout class="sapUiResponsiveMargin">
				<l:content>
					<!--<mvc:XMLView displayBlock="true" height="140px" id="headSelect" viewName="com.broadspectrum.etime.ee.view.HeaderSelection"/>-->
					<!--<mvc:XMLView displayBlock="true" height="100px" id="headSummary" viewName="com.broadspectrum.etime.ee.view.HeaderSummary"/>-->
					<mvc:XMLView displayBlock="true" id="headSelect" viewName="com.broadspectrum.etime.ee.view.HeaderSelection"/>
					<mvc:XMLView displayBlock="true" id="headSummary" viewName="com.broadspectrum.etime.ee.view.HeaderSummary"/>
					<List growing="true" growingScrollToLoad="true" id="master1List" items="{/overviewSet}" noDataText="{i18n]masterListNoDataText}"
						mode="SingleSelectMaster" select="onSelect">
						<items>
							<ObjectListItem id="master1ListItem" title="{Datetxt}" type="{device]/listItemType}">
								<attributes>
									<ObjectAttribute text="{Hourstxt}"/>
									<ObjectAttribute text="{Statustxt}"/>
								</attributes>
							</ObjectListItem>
						</items>
					</List>
				</l:content>
			</l:VerticalLayout>
		</content>
		<dependents>
			<core:Fragment fragmentName="com.broadspectrum.etime.ee.dialogs.BusyDialog" type="XML"></core:Fragment>
		</dependents>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>