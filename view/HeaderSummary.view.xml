<core:View 
	xmlns:core="sap.ui.core" 
	xmlns:c="sap.ui.commons" 
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:l="sap.ui.layout" 
	controllerName="com.broadspectrum.etime.ee.view.HeaderSummary">
	<Panel id="panel" xmlns:sap.m.Panel="sap.m.Panel" width="auto" class="sapUiResponsiveMargin">
	<headerToolbar xmlns:sap.m.Toolbar="sap.m.Toolbar"/>
		<content>
		    <sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="false" layout="ResponsiveLayout" id="__form0">
		            <c:TextView width="100%" id="__input0" value="{Weektxt}" text="{Weektxt}"/>
		            <c:TextView width="100%" id="__input1" value="{Totalhourstxt}" text="{Totalhourstxt}"/>
		            <c:TextView width="100%" id="__input2" value="{Approvedhourstxt}" text="{Approvedhourstxt}"/>
		            <c:TextView width="100%" id="__input3" value="{Rejectedhourstxt}" text="{Rejectedhourstxt}"/>
		    </sap.ui.layout.form:SimpleForm>
	    </content>
	 </Panel>
</core:View>