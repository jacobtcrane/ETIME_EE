<mvc:View
	controllerName="com.broadspectrum.etime.ee.view.NotFound"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page
		title="{i18n>notFoundTitle}"
		enableScrolling="false">
		<content>
        	<MessagePage id="messagePage" showHeader="false" 
        	    text="{i18n>notFoundText}"
        	    icon="sap-icon://question-mark">
        	</MessagePage>		
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>