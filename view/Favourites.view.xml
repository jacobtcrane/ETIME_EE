<core:View controllerName="com.broadspectrum.etime.ee.view.Favourites" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Page navButtonPress="onNavBack" showNavButton="true" title="Favourites">
		<content>
			<List id="favList" items="{/favTableSet}" mode="Delete" delete="handleDelete" 
			    enableBusyIndicator="true" growing="true" >
				<items>
					<StandardListItem title="{Description}" description="{Awart}" 
					    iconDensityAware="false" iconInset="false" />
				</items>
			</List>
		</content>
	</Page>
</core:View>